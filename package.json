{
  "name": "browser-data",
  "version": "1.0.0",
  "description": "Simple demo of using browserslist in a web project",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build": "webpack --config ./webpack.config.js --mode production",
    "browsers": "npx browserslist --coverage '> 1% in my stats' --stats=./browserslist-stats.json",
    "get": "npx browserslist-ga"
  },
  "author": "Matt Unhjem",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "repository": "github:lastoftype/browserslist-demo",
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3"
  },
  "browserslist": [
    "ie >= 8",
    "last 2 versions",
    ">1% in JP",
    ">1% in KR",
    ">1% in US"
  ]
}
